{
  "kind": "collectionType",
  "collectionName": "audiobooks",
  "info": {
    "singularName": "audiobook",
    "pluralName": "audiobooks",
    "displayName": "audiobook"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "publicationDate": {
      "type": "date"
    },
    "duration": {
      "type": "decimal"
    },
    "rating": {
      "type": "integer"
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "audiobooks"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "audiobooks"
    }
  }
}
