{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "book"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string"
    },
    "pages": {
      "type": "integer"
    },
    "rating": {
      "type": "integer"
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "books"
    }
  }
}
